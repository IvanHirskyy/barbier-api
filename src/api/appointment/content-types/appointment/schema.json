{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "singularName": "appointment",
    "pluralName": "appointments",
    "displayName": "Appointment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "barber": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::barber.barber",
      "inversedBy": "appointments"
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "appointments"
    },
    "datetime": {
      "type": "datetime",
      "required": true
    },
    "notes": {
      "type": "richtext",
      "maxLength": 200
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "booked",
        "cancelled",
        "completed"
      ],
      "default": "booked"
    }
  }
}
